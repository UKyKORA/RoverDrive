<launch>
  <group ns="drive">
    <rosparam file="$(find drive)/config/drive_kermit.yaml" />
  </group>
  <node pkg="rosserial_python" type="serial_node.py" name="rosserial_node">
    <param name="port" value="/dev/serial0" />
    <param name="baud" value="256000" />
  </node>
  <include file="$(find raspicam_node)/launch/camerav2_410x308_30fps.launch"/>
</launch>
